{
    "definitions": {},
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "status",
      "data"
    ],
    "properties": {
      "status": {
        "$id": "#/properties/status",
        "type": "string",
        "title": "The Status Schema",
        "default": "",
        "examples": [
          "success"
        ],
        "pattern": "^(.*)$"
      },
      "data": {
        "$id": "#/properties/data",
        "type": "object",
        "title": "The Data Schema",
        "required": [
          "name",
          "salary",
          "age",
          "id"
        ],
        "properties": {
          "name": {
            "$id": "#/properties/data/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "test"
            ],
            "pattern": "^(.*)$"
          },
          "salary": {
            "$id": "#/properties/data/properties/salary",
            "type": "integer",
            "title": "The Salary Schema",
            "default": "",
            "examples": [
              "123"
            ],
            "pattern": "^(.*)$"
          },
          "age": {
            "$id": "#/properties/data/properties/age",
            "type": "integer",
            "title": "The Age Schema",
            "default": "",
            "examples": [
              "23"
            ],
            "pattern": "^(.*)$"
          },
          "id": {
            "$id": "#/properties/data/properties/id",
            "type": "integer",
            "title": "The Id Schema",
            "default": 0,
            "examples": [
              42
            ]
          }
        }
      }
    }
  }